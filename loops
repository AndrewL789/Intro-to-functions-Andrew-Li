for i in range(3):
    print(i)

import turtle
from turtle import * 
t=Turtle()
t.speed(300)

sidelength = 100
rotate = 90
def square(x,y):
    for i in range(4):
        t.forward(x)
        t.left(y)
square(100,90)

def doublesquares(iRange):
     length = 25 
     for i in range (iRange):
         square(length, 90)
         length = length * 2
doublesquares(5)